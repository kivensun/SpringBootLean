<script>
  try {
    if((gon.wait_fork!=undefined && gon.wait_fork==true) || (gon.wait_fetch!=undefined && gon.wait_fetch==true)){
      $('.edit-blob').popup({content:"This project is doing something background, pleace try again later", on: 'hover', delay: { show: 200, hide: 200 }});
      $('.edit-blob').click(function(e){
        e.preventDefault();
      })
    }
  
    setUrl = function(){
      var params = window.location.search
      if (params==undefined || $.trim(params).length==0) return;
      $('span.options').children('.basic').find('a').each(function(index,ele){
        var origin_href = $(ele).attr('href');
        if (origin_href!="#" && origin_href.indexOf('?') == -1){
          $(ele).attr('href',origin_href+params);
        }
      });
    }
  
    setUrl();
  
  
    var $btncopy = $("#copy-text");
    $(document).ready(function() {
      $btncopy.popup({
        content: isCopied ? 'Copied':'Copy to clipboard'
      })
    })
    var isCopied = false;
    $btncopy.on('click', function() {
      try {
        var clipboard = new Clipboard("#copy-text", {
          text: function(trigger) {
            return $("#blob_raw").val();
          }
        });
        var success = document.execCommand('copy');
        if (success) {
          $btncopy.trigger('copied', ['Copied']);
          isCopied = true;
        } else {
          alert("Copy failed. Please copy it manually")
        }
      } catch (err) {
        $btncopy.trigger('copied', ['Copy to clipboard']);
      }
    });
  
    $btncopy.bind('copied', function(event, message) {
      $(this).popup('hide');
      $(this).popup('destroy');
      isCopied = true;
      $(this).popup({content: isCopied ? 'Copied':'Copy to clipboard'})
      $(this).popup('show');
    });
  } catch (error) {
    console.log('blob/action error:' + error);
  }
</script>
